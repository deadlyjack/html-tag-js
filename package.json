{
  "name": "html-tag-js",
  "version": "2.3.0",
  "description": "A simple library for creating and manipulating DOM using JavaScript DOM API.",
  "main": "./dist/tag.js",
  "files": [
    "dist/",
    "jsx/",
    "index.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/tag.js",
      "require": "./dist/tag.js",
      "default": "./dist/tag.js"
    },
    "./Ref": {
      "import": "./dist/ref.js",
      "require": "./dist/ref.js",
      "default": "./dist/ref.js"
    },
    "./ref": {
      "import": "./dist/ref.js",
      "require": "./dist/ref.js",
      "default": "./dist/ref.js"
    },
    "./Reactive": {
      "import": "./dist/reactive.js",
      "require": "./dist/reactive.js",
      "default": "./dist/reactive.js"
    },
    "./reactive": {
      "import": "./dist/reactive.js",
      "require": "./dist/reactive.js",
      "default": "./dist/reactive.js"
    },
    "./dist/polyfill": {
      "import": "./dist/polyfill.js",
      "require": "./dist/polyfill.js",
      "default": "./dist/polyfill.js"
    },
    "./dist/polyfill.js": {
      "import": "./dist/polyfill.js",
      "require": "./dist/polyfill.js",
      "default": "./dist/polyfill.js"
    },
    "./polyfill": {
      "import": "./dist/polyfill.js",
      "require": "./dist/polyfill.js",
      "default": "./dist/polyfill.js"
    },
    "./jsx/syntax-parser.js": {
      "import": "./jsx/syntax-parser.js",
      "require": "./jsx/syntax-parser.js",
      "default": "./jsx/syntax-parser.js"
    },
    "./jsx/jsx-to-tag.js": {
      "import": "./jsx/jsx-to-tag.js",
      "require": "./jsx/jsx-to-tag.js",
      "default": "./jsx/jsx-to-tag.js"
    },
    "./jsx/jsx-to-tag": {
      "import": "./jsx/jsx-to-tag.js",
      "require": "./jsx/jsx-to-tag.js",
      "default": "./jsx/jsx-to-tag.js"
    },
    "./jsx/syntax-parser": {
      "import": "./jsx/syntax-parser.js",
      "require": "./jsx/syntax-parser.js",
      "default": "./jsx/syntax-parser.js"
    },
    "./jsx/tag-loader.js": {
      "import": "./jsx/tag-loader.js",
      "require": "./jsx/tag-loader.js",
      "default": "./jsx/tag-loader.js"
    },
    "./jsx/tag-loader": {
      "import": "./jsx/tag-loader.js",
      "require": "./jsx/tag-loader.js",
      "default": "./jsx/tag-loader.js"
    }
  },
  "scripts": {
    "build": "webpack --mode production",
    "test": "jest ./test",
    "dev": "nodemon --exec 'jest ./test/dev.test.js' --watch ./test --watch ./jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deadlyjack/tag.git"
  },
  "keywords": [
    "html",
    "element",
    "javascript",
    "HTMLElement",
    "DOM Manipulation"
  ],
  "author": "Ajit Kumar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deadlyjack/tag.git/issues"
  },
  "homepage": "https://github.com/deadlyjack/tag.git#readme",
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/types": "^7.27.0",
    "babel-loader": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1"
  },
  "browserslist": "cover 100%,not android < 5"
}
